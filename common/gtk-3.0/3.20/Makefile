install-gtk3themeDATA: $(gtk3theme_DATA)
	@$(NORMAL_INSTALL)
	@list='$(gtk3theme_DATA)'; test -n "$(gtk3themedir)" || list=; \
	if test -n "$$list"; then \
	  echo " $(MKDIR_P) '$(DESTDIR)$(gtk3themedir)'"; \
	  $(MKDIR_P) "$(DESTDIR)$(gtk3themedir)" || exit 1; \
	fi; \
	for p in $$list; do \
	  if test -f "$$p"; then d=; else d="$(srcdir)/"; fi; \
	  echo "$$d$$p"; \
	done | $(am__base_list) | \
	while read files; do \
	  echo " $(INSTALL_DATA) $$files '$(DESTDIR)$(gtk3themedir)'"; \
	  $(INSTALL_DATA) $$files "$(DESTDIR)$(gtk3themedir)" || exit $$?; \
	done

uninstall-gtk3themeDATA:
